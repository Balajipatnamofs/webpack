  "scripts": {
    "start": "NODE_ENV=production node dist/main",
    "start:dev": "node dist/main",
    "watch": "webpack --mode development --watch & nodemon dist/main",
    "build": "rm -rf dist client && webpack --mode production --progress",
    "build:dev": "rm -rf dist client && webpack --mode development --progress",
    "stats": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json dist",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint src/**/*.{js,jsx} --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js)": [
      "npm run lint",
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
